@main:
    LOAD r2, 5  # We're going with factorial of 5 for now
    LOAD r0, r2 # This will hold our answer
    LOAD r1, 1

    initialcheck:
        IFLT r2, 2
        JUMP ret1

    check:
        IFLT r2, 2
        JUMP done
        SUB r2, r2, r1
        MULT r0, r0, r2
        JUMP check    

    ret1:
        LOAD r0, r1 # Special case where the initial value is <2. ie. Factorial(1) or Factorial(0)
    done:

    RETURN